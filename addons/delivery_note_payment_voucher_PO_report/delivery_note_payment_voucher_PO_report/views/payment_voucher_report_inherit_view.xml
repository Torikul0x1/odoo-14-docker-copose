<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_payment_voucher_inherit" inherit_id="account.report_payment_receipt_document">
        <xpath expr="//div[@class='page']" position="before">
            <h2 style="text-align:center;text-decoration: underline;margin-bottom:15px;">Payment Voucher</h2>
        </xpath>

        <xpath expr="//div[@class='page']" position="after">
            <h5 style="margin-top:200px;font-size:14px;">Receiver Signature:_________</h5>
        </xpath>
    </template>
</odoo>
