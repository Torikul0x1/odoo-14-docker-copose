                                    <div class="form-group">
                                        <label for="address" class="control-label">Location/ Address</label>
                                        <input type="text"
                                               name="address"
                                               t-att-value="address"
                                               id="address"
                                               class="form-control" placeholder="Your answer"/>
                                    </div>

                                    <div class="form-check" style="display:none">
                                        <input class="form-check-input" type="radio"
                                               name="option"
                                               id="option_1"
                                               value="Option 1"/>
