<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_account_move_diamond" model="ir.ui.view">
        <field name="name">view.account.move.diamond</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
             <xpath expr="//field[@name='invoice_user_id']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='team_id']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
             <xpath expr="//field[@name='invoice_user_id']" position="after">
                    <field name="multi_salesman" widget="many2many_tags"/>
                    <field name="multi_sales_team" widget="many2many_tags"/>
                </xpath>
        </field>
    </record>

        <record id="view_account_move_tree_diamond" model="ir.ui.view">
        <field name="name">view.account.move.tree.diamond</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_out_invoice_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='invoice_user_id']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='team_id']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
             <xpath expr="//field[@name='invoice_user_id']" position="after">
                    <field name="multi_salesman" widget="many2many_tags"/>
                    <field name="multi_sales_team" widget="many2many_tags"/>
                </xpath>
        </field>
    </record>
</odoo>