<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_invoice_document_inherit_custom_extend" inherit_id="cpabooks_custom_print.report_invoice_document_inherit_custom">
        <xpath expr="//th[@name='th_sr_no']" position="after">
            <th name="th_item_code" class="text-center"><strong>Item Code</strong></th>
        </xpath>
        <xpath expr="//td[@name='account_invoice_line_name']"  position="before">
            <td name="td_item_code">
                <span t-field="line.product_id.item_code"/>
            </td>
        </xpath>
    </template>
</odoo>