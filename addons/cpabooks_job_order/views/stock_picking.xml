<?xml version="1.0" encoding="utf-8"?>
<odoo>
<!--     Inherit Account Move Form View-->
    <record id="view_stock_picking_form_inherit_for_job" model="ir.ui.view">
        <field name="name">stock.picking.form.inherit</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">

            <field name="origin" position="after">
                <field name="job_order_id" invisible="1"/>
                <field name="is_job_order_delivery" invisible="1"/>
            </field>
            <xpath expr="//page[@name='operations']/field[@name='move_ids_without_package']/tree/field[@name='product_qty']" position="after">
                <field name="job_line_id" invisible="1"/>
                <field name="quotation_line_id" invisible="1"/>
            </xpath>

            <xpath expr="//page[@name='operations']" position="attributes">
                <attribute name="string">Delivery</attribute>
            </xpath>
        </field>
    </record>
    <record id="view_stock_picking_tree_inherit_for_job" model="ir.ui.view">
        <field name="name">stock.picking.form.inherit</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.vpicktree"/>
        <field name="arch" type="xml">

            <field name="name" position="after">
                <field name="job_order_id" invisible="1"/>
                <field name="is_job_order_delivery" invisible="1"/>
            </field>
        </field>
    </record>
</odoo>