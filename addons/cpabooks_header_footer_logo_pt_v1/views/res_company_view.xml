<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_company_form_inherit_custom_print" model="ir.ui.view">
        <field name="name">res.company.form.inherit.custom_print</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Report Images" name="partner_logo">
                    <group>
                        <group string="Header">
                            <field name="header_logo" widget="image"/>
                            <field name="is_hide_header_footer"/>
                        </group>
                        <group string="Footer">
                            <field name="partner_logo" widget="image"/>
                            <field name="footer_text"/>
                        </group>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
