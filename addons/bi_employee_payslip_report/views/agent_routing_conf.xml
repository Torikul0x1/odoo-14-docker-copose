<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <record id="agent_routing_code_form" model="ir.ui.view">
            <field name="name">agent.routing.code.form</field>
            <field name="model">agent.routing</field>
            <field name="arch" type="xml">
                <form string="Agent Routing Code">

                <sheet>

                    <group>
                        <group>
                            <field name="code"/>

                        </group>

                    </group>
                    <group>

                        <group>
                            <field name="company_id"/>
                        </group>
                    </group>


                </sheet>
                </form>
            </field>
        </record>

         <record id="agent_routing_code_tree" model="ir.ui.view">
            <field name="name">agent.routing.code.tree</field>
            <field name="model">agent.routing</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="code"/>
                    <field name="company_id"/>
                </tree>

            </field>
         </record>





        <record id="agent_routing_code_action" model="ir.actions.act_window">
            <field name="name">Agent Routing Code Setup</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">agent.routing</field>
            <field name="view_mode">tree</field>
<!--            <field name="target">new</field>-->

        </record>






    <menuitem id="menu_agent_routing_code_setup" name="Agent Routing Code Setup"
            parent="hr_work_entry_contract.menu_hr_payroll_configuration" action="agent_routing_code_action" sequence="5"/>

</odoo>
