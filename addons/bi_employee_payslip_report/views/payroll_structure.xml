<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="hr_payroll_structure_inherited_view_form" model="ir.ui.view">
            <field name="name">hr.payrolL.structure.inherit</field>
            <field name="model">hr.payroll.structure</field>
            <field name="inherit_id" ref="hr_payroll.view_hr_employee_grade_form"/>
            <field name="arch" type="xml">
                <field name="country_id" position="after">
                    <field name="not_rate"/>
                    <field name="hot_rate"/>
                    <field name="company_mol_id"/>
                    <field name="overtime_cal_method"/>
                </field>
                <field name="journal_id" position="after">
<!--                    <field name="weekend"/>-->
                    <field name="weekend_day" widget="many2many_tags"/>
                    <field name="standard_working_hour" widget="float_time"/>
                    <field name="half_day" widget="many2many_tags"/>
                    <field name="half_day_working_hour" widget="float_time"/>
                    <field name="structure_type"/>
                </field>
            </field>
        </record>
</odoo>