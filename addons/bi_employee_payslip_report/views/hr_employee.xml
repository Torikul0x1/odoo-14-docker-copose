<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="hr_employee_form_extend" model="ir.ui.view">
        <field name="name">hr.employee.form.extend</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr.view_employee_form"/>
        <field name="arch" type="xml">
<!--            <xpath expr="//header" position="inside">-->
<!--                <button name="create_contract" string="Create Contract" type="object" groups="hr.group_hr_manager"-->
<!--                        class="btn btn-primary"/>-->
<!--            </xpath>-->

            <xpath expr="//field[@name='bank_account_id']" position="after">
                <field name="agent_routing_code"/>
                <field name="emp_mol_id"/>
            </xpath>
            <xpath expr="//field[@name='coach_id']" position="after">
