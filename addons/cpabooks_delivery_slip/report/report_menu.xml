<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <!--region HEADERLESS REPORT-->
        <template id="headerless_header">
            <div t-attf-class="header">
            </div>
        </template>
        <template id="footerless_footer">
            <div class="footer">
            </div>
        </template>
        <record id="delivery_headerless_report" model="ir.actions.report">
            <field name="name">Delivery Slip (Without Header &amp; Footer)</field>
            <field name="model">stock.picking</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">cpabooks_delivery_slip.custom_delivery_slip_without_header</field>
            <field name="report_file">cpabooks_delivery_slip.custom_delivery_slip_without_header</field>
            <field name="binding_model_id" ref="stock.model_stock_picking"/>
            <field name="binding_type">report</field>
        </record>
        <!--endregion-->
    <record id="stock.action_report_picking" model="ir.actions.report">
        <field name="binding_model_id" eval="False"/>
    </record>
    <record id="stock.action_label_transfer_template_pdf" model="ir.actions.report">
        <field name="binding_model_id" eval="False"/>
    </record>
    <record id="stock.action_label_transfer_template_zpl" model="ir.actions.report">
        <field name="binding_model_id" eval="False"/>
    </record>

     <menuitem id="menu_delivery_slip"
              parent="account.menu_finance_payables"
              sequence="3"
              action="stock.stock_picking_action_picking_type"
              name="GRN"/>
</odoo>
