<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="custom_delivery_slip_without_header">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="cpabooks_delivery_slip.headerless_header"/>
                <t t-call="cpabooks_delivery_slip.footerless_footer"/>
                <div>
                    <t t-call="cpabooks_delivery_slip.report_delivery_document_custom"/>
                </div>
            </t>
        </t>
    </template>
</odoo>