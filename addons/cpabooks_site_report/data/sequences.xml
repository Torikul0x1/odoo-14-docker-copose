<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="daily_site_report_seq" model="ir.sequence">
            <field name="name">Daily Site Report sequence</field>
            <field name="code">daily.site.report</field>
            <field name="prefix">DR# %(year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">5</field>
            <field eval="False" name="company_id"/>
