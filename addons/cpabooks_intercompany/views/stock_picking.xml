<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_stock_picking_form_extend" model="ir.ui.view">
        <field name="name">view.stock.picking.form.extend</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//header/button[@name='action_cancel']" position="after">
                <button name="reserve_qty" string="Reserved Qty" type="object" class="btn-primary" attrs="{'invisible':['|',('state','in',('done','cancel')),('location_dest_id','!=',5)]}"/>
            </xpath>


        </field>
    </record>
</odoo>