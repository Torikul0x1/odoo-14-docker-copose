<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="custom_invoice_without_header">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="cpabooks_custom_print.headerless_header"/>
                <t t-call="cpabooks_custom_print.footerless_footer"/>
                <div>
                    <t t-call="cpabooks_custom_print.report_invoice_document_inherit_custom"/>
                </div>
            </t>
        </t>
    </template>
</odoo>
