<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
<template id="purchase_order">
    <t t-call="web.report_layout">
         <t t-foreach="docs" t-as="o">
			<t t-set="o" t-value="o.with_context({'lang':o.partner_id.lang})" />
            <t t-set="style" t-value="o.po_style or res_company.df_style or user.env.ref('professional_templates_v1.df_style_for_all_reports')"/>
            <t t-if="style.template_po.key == 'professional_templates_v1.PO_1_document'" t-call="professional_templates_v1.PO_1_document" t-lang="o.partner_id.lang"/>
            <t t-if="style.template_po.key == 'professional_templates_v1.PO_2_document'" t-call="professional_templates_v1.PO_2_document" t-lang="o.partner_id.lang"/>
            <t t-if="style.template_po.key == 'professional_templates_v1.PO_3_document'" t-call="professional_templates_v1.PO_3_document" t-lang="o.partner_id.lang"/>
            <t t-if="style.template_po.key == 'professional_templates_v1.PO_4_document'" t-call="professional_templates_v1.PO_4_document" t-lang="o.partner_id.lang"/>
            <t t-if="style.template_po.key == 'professional_templates_v1.PO_5_document'" t-call="professional_templates_v1.PO_5_document" t-lang="o.partner_id.lang"/>
            <t t-if="style.template_po.key == 'professional_templates_v1.PO_6_document'" t-call="professional_templates_v1.PO_6_document" t-lang="o.partner_id.lang"/>
            <t t-if="style.template_po.key == 'professional_templates_v1.PO_7_document'" t-call="professional_templates_v1.PO_7_document" t-lang="o.partner_id.lang"/>
            <t t-if="style.template_po.key == 'professional_templates_v1.PO_8_document'" t-call="professional_templates_v1.PO_8_document" t-lang="o.partner_id.lang"/>
            <t t-if="style.template_po.key == 'professional_templates_v1.PO_9_document'" t-call="professional_templates_v1.PO_9_document" t-lang="o.partner_id.lang"/>
            <t t-if="style.template_po.key == 'professional_templates_v1.PO_10_document'" t-call="professional_templates_v1.PO_10_document" t-lang="o.partner_id.lang"/>
            <t t-if="style.template_po.key == 'professional_templates_v1.PO_11_document'" t-call="professional_templates_v1.PO_11_document" t-lang="o.partner_id.lang"/>
            <t t-if="style.template_po.key == 'professional_templates_v1.PO_12_document'" t-call="professional_templates_v1.PO_12_document" t-lang="o.partner_id.lang"/>
            <t t-if="style.template_po.key == 'professional_templates_v1.PO_13_document'" t-call="professional_templates_v1.PO_13_document" t-lang="o.partner_id.lang"/>
            <t t-if="style.template_po.key == 'professional_templates_v1.PO_14_document'" t-call="professional_templates_v1.PO_14_document" t-lang="o.partner_id.lang"/>
            <t t-if="not style" t-call="purchase.report_purchaseorder_document" t-lang="o.partner_id.lang"/>
         </t>
    </t>
</template>
</data>
</odoo>
