<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
<template id="rfq">
    <t t-call="web.report_layout">
        <t t-foreach="docs" t-as="o">
		<t t-set="o" t-value="o.with_context({'lang':o.partner_id.lang})" />
        <t t-set="style" t-value="o.rfq_style or res_company.df_style or user.env.ref('professional_templates_v1.df_style_for_all_reports')"/>
	<t t-if="style.template_rfq.key =='professional_templates_v1.RFQ_1_document'" t-call="professional_templates_v1.RFQ_1_document" t-lang="o.partner_id.lang"/>
	<t t-if="style.template_rfq.key =='professional_templates_v1.RFQ_2_document'" t-call="professional_templates_v1.RFQ_2_document" t-lang="o.partner_id.lang"/>
	<t t-if="style.template_rfq.key =='professional_templates_v1.RFQ_3_document'" t-call="professional_templates_v1.RFQ_3_document" t-lang="o.partner_id.lang"/>
	<t t-if="style.template_rfq.key =='professional_templates_v1.RFQ_4_document'" t-call="professional_templates_v1.RFQ_4_document" t-lang="o.partner_id.lang"/>
	<t t-if="style.template_rfq.key =='professional_templates_v1.RFQ_5_document'" t-call="professional_templates_v1.RFQ_5_document" t-lang="o.partner_id.lang"/>
	<t t-if="style.template_rfq.key =='professional_templates_v1.RFQ_6_document'" t-call="professional_templates_v1.RFQ_6_document" t-lang="o.partner_id.lang"/>
	<t t-if="style.template_rfq.key =='professional_templates_v1.RFQ_7_document'" t-call="professional_templates_v1.RFQ_7_document" t-lang="o.partner_id.lang"/>
	<t t-if="style.template_rfq.key =='professional_templates_v1.RFQ_8_document'" t-call="professional_templates_v1.RFQ_8_document" t-lang="o.partner_id.lang"/>
	<t t-if="style.template_rfq.key =='professional_templates_v1.RFQ_9_document'" t-call="professional_templates_v1.RFQ_9_document" t-lang="o.partner_id.lang"/>
	<t t-if="style.template_rfq.key =='professional_templates_v1.RFQ_10_document'" t-call="professional_templates_v1.RFQ_10_document" t-lang="o.partner_id.lang"/>
	<t t-if="style.template_rfq.key =='professional_templates_v1.RFQ_11_document'" t-call="professional_templates_v1.RFQ_11_document" t-lang="o.partner_id.lang"/>
	<t t-if="style.template_rfq.key =='professional_templates_v1.RFQ_12_document'" t-call="professional_templates_v1.RFQ_12_document" t-lang="o.partner_id.lang"/>
	<t t-if="not style" t-call="purchase.report_purchasequotation_document" t-lang="o.partner_id.lang"/>
     </t>
    </t>
</template>
</data>
</odoo>
