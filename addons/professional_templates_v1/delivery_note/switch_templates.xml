<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
<template id="report_deliveryslip">
    <t t-call="web.report_layout">
        <t t-foreach="docs" t-as="o">
		<t t-set="o" t-value="o.with_context({'lang':o.partner_id.lang})" />
        <t t-set="style" t-value="o.dn_style or res_company.df_style or user.env.ref('professional_templates_v1.df_style_for_all_reports')"/>
	<t t-if="style.template_dn.key == 'professional_templates_v1.DN_1_document'" t-call="professional_templates_v1.DN_1_document" t-lang="o.partner_id.lang"/>
	<t t-if="style.template_dn.key == 'professional_templates_v1.DN_2_document'" t-call="professional_templates_v1.DN_2_document" t-lang="o.partner_id.lang"/>
	<t t-if="style.template_dn.key == 'professional_templates_v1.DN_3_document'" t-call="professional_templates_v1.DN_3_document" t-lang="o.partner_id.lang"/>
	<t t-if="style.template_dn.key == 'professional_templates_v1.DN_4_document'" t-call="professional_templates_v1.DN_4_document" t-lang="o.partner_id.lang"/>
	<t t-if="style.template_dn.key == 'professional_templates_v1.DN_5_document'" t-call="professional_templates_v1.DN_5_document" t-lang="o.partner_id.lang"/>
	<t t-if="style.template_dn.key == 'professional_templates_v1.DN_6_document'" t-call="professional_templates_v1.DN_6_document" t-lang="o.partner_id.lang"/>
	<t t-if="style.template_dn.key == 'professional_templates_v1.DN_7_document'" t-call="professional_templates_v1.DN_7_document" t-lang="o.partner_id.lang"/>
