	<t t-if="style.template_dn.key == 'professional_templates_v1.DN_10_document'" t-call="professional_templates_v1.DN_10_document" t-lang="o.partner_id.lang"/>
	<t t-if="style.template_dn.key == 'professional_templates_v1.DN_11_document'" t-call="professional_templates_v1.DN_11_document" t-lang="o.partner_id.lang"/>
	<t t-if="style.template_dn.key == 'professional_templates_v1.DN_12_document'" t-call="professional_templates_v1.DN_12_document" t-lang="o.partner_id.lang"/>
	<t t-if="style.template_dn.key == 'professional_templates_v1.DN_13_document'" t-call="professional_templates_v1.DN_13_document" t-lang="o.partner_id.lang"/>
	<t t-if="not style" t-call="stock.report_delivery_document" t-lang="o.partner_id.lang"/>
     </t>
    </t>
</template>
</data>
</odoo>
