<odoo>
   <record id="view_dev_config_form" model="ir.ui.view">
        <field name="name">view.dev.config.form</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="50"/>
        <field name="inherit_id" ref="hr.res_config_settings_view_form" />
        <field name="arch" type="xml">
	        <div data-key="hr" position="inside">
	            <h2>Employee Document Expiry</h2>
                <div class="col-xs-12 col-md-6 o_setting_box" data-key="hr">
                    <div class="o_setting_left_pane">
                        <group>
                            <field name="expiry_before_days"/>
                        </group>
                    </div>
                </div>
             </div>
		</field>
	</record>


</odoo>
