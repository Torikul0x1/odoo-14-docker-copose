<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!--Paper Format for QC Label-->
        <record id="paperformat_qc_label" model="report.paperformat">
            <field name="name">Dymo Label Sheet</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">60</field>
            <field name="page_width">90</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">1</field>
            <field name="margin_bottom">1</field>
            <field name="margin_left">0</field>
            <field name="margin_right">0</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">0</field>
            <field name="dpi">96</field>
        </record>

        <record id="report_qc_label" model="ir.actions.report">
            <field name="name">QC Label</field>
            <field name="model">quality.check</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">cpabooks_switchgear_custom.report_qc_label_sw</field>
            <field name="report_file">cpabooks_switchgear_custom.report_qc_label</field>
            <field name="paperformat_id" ref="cpabooks_switchgear_custom.paperformat_qc_label"/>
            <field name="print_report_name">'QC Label - %s' % (object.product_id.name)</field>
            <field name="binding_model_id" ref="quality.model_quality_check"/>
            <field name="binding_type">report</field>
        </record>

