<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="res_config_settings_view_form_2" model="ir.ui.view">
        <field name="name">
            res.config.settings.view.form.inherit2.ir_attachment_s3
        </field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="12"/>
        <field name="inherit_id" ref="base.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='s3_upload_existing']" position="after">
                <button name="restore_s3_attachments" type="object"
                        string="Restore Attachments"/>
            </xpath>
        </field>
    </record>
</odoo>
